{
  "git": {
    "init": "git clone {url} {workspaceDir}/{name}",
    "refresh": "cd {workspaceDir}/{name}; git pull",
    "save": "git push origin master"
  },
  "svn": {
    "init": "svn checkout {url} {workspaceDir}/{name}",
    "refresh": "cd {workspaceDir}/{name}; svn up",
    "check": "svn diff",
    "save": "svn commit -m \"Commited by Repoman\""
  }
}